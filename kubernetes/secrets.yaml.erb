apiVersion: v1
kind: Secret
metadata:
  name: keycloak
type: Opaque
stringData:
  secret: <%= keycloak_secret %>
  encryption_key: <%= keycloak_encryption_key %>
---
apiVersion: v1
kind: Secret
metadata:
  name: irclogger
type: Opaque
stringData:
  database: <%= database %>
  nickserv_password: <%= nickserv_password %>
